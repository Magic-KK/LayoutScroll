<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EasyLayoutScroll">
        <!--布局切换间隔-->
        <attr name="interval" format="integer"></attr>
        <!--动画执行时间-->
        <attr name="duration" format="integer"></attr>
        <!--设置动画渐变效果-->
        <attr name="gradient" format="boolean"></attr>
    </declare-styleable>
</resources>